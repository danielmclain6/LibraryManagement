<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Library Manager</display-name>
  <servlet>
  	<servlet-name>LibrarianServices</servlet-name>
  	<display-name>LibrarianServices</display-name>
  	<description></description>
  	<servlet-class>com.cognixia.jump.library.servlets.LibrarianServices</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>LibrarianServlet</servlet-name>
  	<display-name>LibrarianServlet</display-name>
  	<description></description>
  	<servlet-class>com.cognixia.jump.library.servlets.LibrarianServlet</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>Login</servlet-name>
  	<display-name>Login</display-name>
  	<description></description>
  	<servlet-class>com.cognixia.jump.library.servlets.Login</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>BookCheckout</servlet-name>
  	<display-name>BookCheckout</display-name>
  	<description></description>
  	<servlet-class>com.cognixia.jump.library.servlets.BookCheckout</servlet-class>
  </servlet>
  <servlet-mapping>
  	<servlet-name>LibrarianServices</servlet-name>
  	<url-pattern>/LibrarianServices</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>LibrarianServlet</servlet-name>
  	<url-pattern>/LibrarianServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>Login</servlet-name>
  	<url-pattern>/Login</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
  	<servlet-name>BookCheckout</servlet-name>
  	<url-pattern>/BookCheckout</url-pattern>
  </servlet-mapping>
</web-app>
